@model IEnumerable<EntityLayer.Concrete.UserActivity>
@{
    ViewData["Title"] = "MyActivities";
    Layout = "~/Views/Shared/IndividualLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.1/js/bootstrap.min.js"></script>
</head>
<body>
    <div class="container">
        <table class="table table-bordered">
            <thead>
                <tr>
                    <th>Etkinlik Adı</th>
                    <th>Etkinlik Açıklaması</th>
                    <th>Etkinlik Tarihi</th>
                    <th>Etkinlik Yeri</th>
                    
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th>@item.Activity.ActivityName</th>
                        <th>@item.Activity.Explanation</th>
                        <th>@item.Activity.Date</th>
                        <th>@item.Activity.Address</th>
                    </tr>
                }
            </tbody>

        </table>

    </div>
</body>
</html>

